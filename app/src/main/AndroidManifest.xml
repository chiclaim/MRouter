<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chiclaim.modularization">

    <application
        android:name="com.chiclaim.modularization.sample.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".sample.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.chiclaim.modularization.sample.ActivityManagerActivity"
            android:label="ActivityManager"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.chiclaim.modularization.sample.AActivity"
            android:label="AActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.chiclaim.modularization.sample.BActivity"
            android:label="BActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.chiclaim.modularization.sample.CActivity"
            android:label="CActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.chiclaim.modularization.sample.DActivity"
            android:label="DActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.chiclaim.modularization.sample.NoAutowiredActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.chiclaim.modularization.sample.TargetActivity"
            android:label="TargetActivity"
            android:screenOrientation="portrait" />

    </application>

</manifest>